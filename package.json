{
  "name": "style-variance-authority",
  "version": "0.0.9",
  "description": "Style Variance Authority",
  "author": "Tomer Danan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dananz/sva.git"
  },
  "homepage": "https://github.com/dananz/sva#readme",
  "bugs": "https://github.com/dananz/sva/issues",
  "sideEffects": false,
  "type": "module",
  "keywords": [
    "Style Variance Authority",
    "style-variance-authority",
    "sva",
    "cva",
    "variants"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint . && biome check .",
    "lint:fix": "automd && eslint . --fix && biome check --write .",
    "prepack": "pnpm build",
    "play": "jiti playground",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/node": "^20.14.9",
    "@vitest/coverage-v8": "^1.6.0",
    "automd": "^0.3.7",
    "changelogen": "^0.5.5",
    "eslint": "^9.6.0",
    "eslint-config-unjs": "^0.3.2",
    "jiti": "^1.21.6",
    "typescript": "^5.5.2",
    "unbuild": "^2.0.0",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "csstype": "^3.1.3"
  },
  "packageManager": "pnpm@9.0.6"
}
